            "typeVersion": 1,
            "position": [
                250,
                300
            ]
        },
        {
            "parameters": {
                "jsCode": "const { normalizePayload } = require('/data/workflows/sync.js');\nconst input = items[0].json;\nreturn [ { json: normalizePayload('youtrack', input) } ];"
            },
            "name": "Normalize Payload",
            "type": "n8n-nodes-base.code",
            "typeVersion": 1,
            "position": [
                450,
                300
            ]
