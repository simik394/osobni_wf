            "parameters": {
                "method": "POST",
                "url": "http://halvarm:8080/webhook",
                "sendQuery": true,
                "queryParameters": {
                    "parameters": [
                        {
                            "name": "secret",
                            "value": "YOUR_SECRET_TOKEN"
                        }
                    ]
                },
                "sendBody": true,
                "contentType": "json",
                "bodyParameters": {
                    "parameters": [
                        {
                            "name": "data",
                            "value": "={{ $json }}"
                        }
                    ]
                },
                "options": {}
            },
            "name": "Forward to Proxy",
            "type": "n8n-nodes-base.httpRequest",
            "typeVersion": 1,
            "position": [
                650,
                300
            ]
        }
    ],
    "connections": {
        "YouTrack Webhook": {
            "main": [
                [
                    {
                        "node": "Normalize Payload",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Normalize Payload": {
            "main": [
                [
                    {
