{
  "name": "rsrch",
  "version": "1.0.35",
  "description": "Automated researcher agent using Perplexity AI and Gemini",
  "main": "src/index.ts",
  "bin": {
    "rsrch": "dist/cli.js"
  },
  "scripts": {
    "rsrch": "ts-node src/cli.ts",
    "auth": "ts-node src/cli.ts auth",
    "login": "HEADLESS=false ts-node src/cli.ts login",
    "start": "node dist/cli.js",
    "serve": "ts-node src/server.ts",
    "debug:notebook": "ts-node src/debug-notebooklm.ts",
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "docs:api": "ts-node scripts/generate_api_docs.ts > docs/API_REFERENCE.qmd",
    "docs:cli": "ts-node scripts/generate_cli_docs.ts > docs/CLI_REFERENCE.qmd",
    "docs:config": "ts-node scripts/generate_config_docs.ts > docs/CONFIG_REFERENCE.qmd",
    "docs:arch": "ts-node scripts/generate_architecture.ts > docs/ARCHITECTURE.qmd",
    "docs:render": "quarto render docs/index.qmd && quarto render docs/API_REFERENCE.qmd && quarto render docs/CLI_REFERENCE.qmd && quarto render docs/CONFIG_REFERENCE.qmd && quarto render docs/ARCHITECTURE.qmd",
    "docs:all": "npm run docs:api && npm run docs:cli && npm run docs:config && npm run docs:arch && npm run docs:render"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@agents/shared": "file:../shared",
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "falkordb": "^6.5.1",
    "ioredis": "^5.9.2",
    "js-yaml": "^4.1.1",
    "langfuse": "^3.38.6",
    "pino": "^8.19.0",
    "playwright": "^1.57.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-core": "^21.5.0",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "yaml": "^2.8.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.27",
    "@vitest/ui": "^1.6.0",
    "msw": "^2.2.14",
    "pino-pretty": "^10.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.3",
    "vitest": "^1.6.0"
  }
}